           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:layout_gravity="right"
           android:layout_marginTop="405dp"
           android:layout_marginStart="7dp"
           android:layout_weight="1"
           android:fontFamily="@font/titillium_web"
           android:text="--------------------------"
           android:textStyle="bold"
           android:textColor="#ffffff"
           android:textSize="30sp" />
       <Button
           android:id="@+id/title3"
           android:layout_width="240dp"
           android:layout_height="70dp"
           android:background="@drawable/dashbluebutton"
           android:layout_gravity="left"
           android:layout_marginStart="0dp"
           android:layout_marginTop="440dp"
           android:layout_weight="1"
           android:fontFamily="@font/titillium_web"
           android:text="TOTAL \nGESTIONES"
           android:elevation="30dp"
           android:paddingBottom="5dp"
           android:textAlignment="center"
           android:textColor="#202156"
           android:textSize="20sp"
           android:textStyle="bold" />
       <Button
           android:id="@+id/totalGestiones"
           android:layout_width="130dp"
           android:layout_height="70dp"
           android:background="@drawable/cceleste"
           android:layout_gravity="left"
           android:layout_marginTop="440dp"
           android:layout_marginStart="245dp"
           android:layout_weight="1"
           android:paddingRight="70dp"
           android:fontFamily="@font/titillium_web"
           android:text="0"
           android:onClick="consultarAcciones"
           android:paddingBottom="5dp"
           android:textColor="#202156"
           android:textSize="50sp"
           android:textStyle="bold" />
    <!--   <Button
           android:id="@+id/button3"
           android:layout_width="190dp"
           android:layout_height="55dp"
           android:background="@drawable/action"
           android:layout_gravity="center"
           android:layout_marginTop="188dp"
           android:onClick="goIngresoOd"
           android:elevation="30dp"
           android:fontFamily="@font/titillium_web"
           android:text="NUEVA GESTION"
           android:textSize="18sp" />-->
        <TextView
            android:id="@+id/transmisiones"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="250dp"
            android:layout_weight="1"
            android:fontFamily="@font/titillium_web"
            android:text="ESTADO: SINCRONIZADO"
            android:textColor="#0E004D"
            android:textSize="15sp" />
        <TextView
            android:id="@+id/version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="270dp"
            android:layout_weight="1"
            android:fontFamily="@font/titillium_web"
            android:text="@string/version"
            android:textColor="#0E004D"
            android:textSize="15sp" />
