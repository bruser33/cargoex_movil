        <Button
            android:id="@+id/title2"
            android:layout_width="240dp"
            android:layout_height="55dp"
            android:elevation="60dp"
            android:background="@drawable/dashredbutton"
            android:layout_gravity="left"
            android:layout_marginStart="0dp"
            android:layout_marginTop="170dp"
            android:layout_weight="1"
            android:onClick="goDevoluciones"
            android:fontFamily="@font/titillium_web"
            android:text="NO ENTREGADOS"
            android:textColor="#FF0000"
            android:textSize="20sp"
            android:textStyle="bold" />
        <Button
            android:id="@+id/title4"
            android:layout_width="240dp"
            android:layout_height="55dp"
            android:background="@drawable/dashorangebutton"
            android:layout_gravity="left"
            android:layout_marginStart="0dp"
            android:layout_marginTop="260dp"
            android:elevation="60dp"
            android:layout_weight="1"
            android:onClick="goRetiros"
            android:fontFamily="@font/titillium_web"
            android:text="RETIROS"
            android:textColor="#cc6600"
            android:textSize="20sp"
            android:textStyle="bold" />

        <Button
